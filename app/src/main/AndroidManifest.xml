<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exam.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.exam.com.activities.Home"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.exam.com.activities.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.exam.com.activities.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.exam.com.activities.UsersListActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.exam.com.activities.Food_activity" />
        <activity android:name="com.exam.com.activities.Drink_activity" />
        <activity android:name="com.exam.com.activities.Recomendations" />
        <activity android:name="com.exam.com.activities.HowToPay"></activity>
    </application>

</manifest>